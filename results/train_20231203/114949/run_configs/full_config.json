{
    "dataset_name": "g2net-gravitational-wave-detection",
    "ifos": [
        "LIGO Hanford",
        "LIGO Livingston",
        "Virgo"
    ],
    "total_datapoints": 560000,
    "input_channels": 3,
    "seq_len": 4096,
    "n_classes": 2,
    "z_norm": true,
    "highpass": true,
    "whiten": false,
    "scale": false,
    "bandpass": true,
    "epochs": 100,
    "batch_size": 128,
    "num_batches": 4375,
    "optimizer": "sgd",
    "learning_rate": 0.1,
    "lr_scheduler": "step",
    "stop_early": true,
    "accumulate_grad_batches": 3,
    "num_hidden_units": 10,
    "levels": 8,
    "kernel_size": 5,
    "nesterov": true,
    "momentum": 0.9,
    "weight_decay": 0.0,
    "lr_scheduler__step_size": 10,
    "lr_scheduler__gamma": 0.1,
    "stop_early__monitor": "val_loss",
    "stop_early__mode": "min",
    "stop_early__patience": 5,
    "use_gpu": 1,
    "model_name": "TCN",
    "sample_size": 560000
}